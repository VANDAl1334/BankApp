<Page x:Class="UserApp.Window.Main.PgSupport"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:UserApp.Window.Main"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="750"
      Title="PgSupport">
    <Grid>
        <Grid Margin="170 0 170 47" Background="Gray">
        </Grid>
        <StackPanel Margin="0 0 0 15" VerticalAlignment="Bottom" HorizontalAlignment="Center" Orientation="Horizontal">
            <TextBox Width="359" Margin="0 0 19 0" TextWrapping="WrapWithOverflow" BorderBrush="Brown" BorderThickness="1.5" Height="30" FontFamily="Times New Roman" FontSize="16" Style="{StaticResource MaterialDesignTextBox}"/>
            <Button Width="32" Height="32" Style="{StaticResource MaterialDesignIconButton}">
                <Image Source="/Window/Main/Sent.ico"/>
            </Button>
        </StackPanel>

    </Grid>
</Page>
