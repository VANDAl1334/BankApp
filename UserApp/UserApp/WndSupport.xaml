<Window x:Class="UserApp.WndSupport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UserApp"
        mc:Ignorable="d"
        Title="Support" Height="450" Width="500" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen">
    <Grid>
        <DockPanel VerticalAlignment="Bottom">
            <TextBox Width="452" BorderBrush="Gray" BorderThickness="1.5" Height="30" FontFamily="Times New Roman" FontSize="16" Style="{StaticResource MaterialDesignTextBox}"/>
            <Button Width="32" Height="32" Style="{StaticResource MaterialDesignIconButton}">
                <Image Source="/Send.png"/>
            </Button>
        </DockPanel>
    </Grid>
</Window>
